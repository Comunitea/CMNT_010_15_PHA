<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="product_stock_unsafety_extended_tree" model="ir.ui.view">
            <field name="name">Product stock unsafety extended tree view</field>
            <field name="model">product.stock.unsafety</field>
            <field name="inherit_id" ref="product_stock_unsafety.view_product_stock_unsafety_tree"/>
            <field name="arch" type="xml">
                <field name="max_fixed" position="after">
                    <field name="stock_by_day"/>
                </field>
                <tree position="attributes">
                    <attribute name="default_order">stock_by_day</attribute>
                </tree>
            </field>
        </record>
    </data>
</openerp>
